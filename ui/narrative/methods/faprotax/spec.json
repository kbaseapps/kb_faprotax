{
    "ver": "0.0.1",
    "authors": [
        "swwang",
        "seanjungbluth"
    ],
    "contact": "",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "input_upa",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseMatrices.AmpliconMatrix", "KBaseSearch.GenomeSet"]
            }
        },{
            "id": "tax_field",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "field_type": "textsubdata",
            "textsubdata_options" : {
                "show_src_obj": false,
                "allow_custom": true,
                "multiselection": false,
                "subdata_selection": {
                    "parameter_id": "input_upa",
                    "path_to_subdata": [
                        "row_attributemapping_ref",
                        "<WSREF>",
                        "attributes"
                    ],
                    "subdata_included": [
                        "attributes/[*]/attribute",
                        "attributes/[*]/source",
                        "row_attributemapping_ref"
                    ],
                    "selection_id": "attribute",
                    "selection_description": [
                        "attribute",
                        "source"
                    ],
                    "description_template": "source: {{source}}"
                }
            }
        },{
            "id": "output_amplicon_matrix_name",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_type": ["KBaseMatrices.AmpliconMatrix"],
                "is_output_name": true
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_faprotax",
            "method": "run_FAPROTAX",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "input_upa",
                    "target_property": "input_upa",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "tax_field",
                    "target_property": "tax_field"
                },{
                    "input_parameter": "output_amplicon_matrix_name",
                    "target_property": "output_amplicon_matrix_name"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
